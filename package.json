    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.2.0",
    "jest-vue": "^0.8.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
